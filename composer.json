{
    "name": "micoli/symfony-cartography",
    "license": "MIT",
    "type": "library",
    "description": "Symfony cartography bundle",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "graphp/graph": "^0.9.3",
        "micoli/multitude": "0.*",
        "nikic/php-parser": "^4.15",
        "symfony/asset": "^6.2",
        "symfony/console": "^6.2",
        "symfony/doctrine-messenger": "^6.2",
        "symfony/expression-language": "^6.2",
        "symfony/flex": "^2.0",
        "symfony/framework-bundle": "^6.2",
        "symfony/messenger": "^6.2",
        "symfony/property-access": "^6.2",
        "symfony/runtime": "^6.2",
        "symfony/serializer": "^6.2",
        "symfony/string": "^6.2",
        "symfony/translation": "^6.2",
        "symfony/twig-bundle": "^6.2",
        "symfony/validator": "^6.2",
        "symfony/yaml": "^6.2",
        "thecodingmachine/safe": "^2.4",
        "twig/extra-bundle": "^3.3",
        "twig/intl-extra": "^3.3"
    },
    "require-dev": {
        "ext-pdo_sqlite": "*",
        "ext-simplexml": "*",
        "dama/doctrine-test-bundle": "^7.0",
        "doctrine/dbal": "^3.1",
        "doctrine/doctrine-bundle": "^2.8",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.11",
        "friendsofphp/php-cs-fixer": "3.*",
        "league/commonmark": "^2.1",
        "phpdocumentor/reflection": "~5.0",
        "phpunit/phpunit": "^10.0",
        "symfony/apache-pack": "^1.0",
        "symfony/browser-kit": "^6.2",
        "symfony/css-selector": "^6.2",
        "symfony/debug-bundle": "^6.2",
        "symfony/dotenv": "^6.2",
        "symfony/form": "^6.2",
        "symfony/html-sanitizer": "^6.2",
        "symfony/http-client": "^6.2",
        "symfony/intl": "^6.2",
        "symfony/mailer": "^6.2",
        "symfony/maker-bundle": "^1.36",
        "symfony/monolog-bundle": "^3.7",
        "symfony/phpunit-bridge": "^6.2",
        "symfony/polyfill-intl-messageformatter": "^1.12",
        "symfony/property-info": "^6.2",
        "symfony/security-bundle": "^6.2",
        "symfony/stopwatch": "^6.2",
        "symfony/ux-live-component": "^2.6",
        "symfony/web-profiler-bundle": "^6.2",
        "symfony/webpack-encore-bundle": "^1.13",
        "twig/markdown-extra": "^3.3",
        "vimeo/psalm": "^5.7"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "platform": {
            "php": "8.2"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Micoli\\SymfonyCartography\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/",
            "App\\": "TestApplication/src/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "php-cs-fixer": "PHP_CS_FIXER_IGNORE_ENV=1 php-cs-fixer"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "6.2.*"
        }
    }
}
